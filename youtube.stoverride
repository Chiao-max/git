name: |-
  YouTube
desc: |-
  YouTube去广告

http:

  mitm:
    - "youtubei.googleapis.com"

  script:
    - match: ^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/(browse|next)
      name: "youtube_4"
      type: response
      require-body: true
      max-size: -1
      binary-mode: true
      timeout: 60

script-providers:
  "youtube_4":
    url: https://raw.githubusercontent.com/app2smile/rules/master/js/youtube.js
    interval: 86400
